@model IEnumerable<Inventory.Models.Chart>

@{
    var myChart = new Chart(width: 900, height: 400, theme: ChartTheme.Green)
        .AddTitle("Monthwise Orders - Last 6 months")
        .AddSeries(
            name: "Month",
            xValue: Model,
            xField: "MonthName",
            yValues: Model,
            yFields: "NoOfOrders")
        .Write();
}
